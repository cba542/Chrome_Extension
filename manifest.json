{
  "manifest_version": 3,
  "name": "Simple Chrome Extension Example",
  "version": "1.0",
  "description": "A simple Chrome extension example",
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {}
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
